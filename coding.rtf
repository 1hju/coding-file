\pard\b0 //add new key=>value to the HTML5 storage\par
function SaveItem() \{\par
var name = document.forms.ShoppingList.name.value;\par
var data = document.forms.ShoppingList.data.value;\par
localStorage.setItem(name, data);\par
doShowAll();\par
\}\par
//------------------------------------------------------------------------------\par
//change an existing key=>value in the HTML5 storage\par
function ModifyItem() \{\par
var name1 = document.forms.ShoppingList.name.value;\par
var data1 = document.forms.ShoppingList.data.value;\par
//check if name1 is already exists\par
//check if key exists\par
if (localStorage.getItem(name1) !=null)\par
\{\par
//update\par
localStorage.setItem(name1,data1);\par
document.forms.ShoppingList.data.value = localStorage.getItem(name1);\par
\}\par
doShowAll();\par
\}\par
//-------------------------------------------------------------------------\par
//delete an existing key=>value from the HTML5 storage\par
function RemoveItem() \{\par
var name = document.forms.ShoppingList.name.value;\par
document.forms.ShoppingList.data.value = localStorage.removeItem(name);\par
doShowAll();\par
\}\par
//-------------------------------------------------------------------------------------\par
//restart the local storage\par
function ClearAll() \{\par
\f1\fs22 Page 62\par
\f3\fs28 localStorage.clear();\par
doShowAll();\par
\}\par
//--------------------------------------------------------------------------------------\par
// dynamically populate the table with shopping list items\par
//below step can be done via PHP and AJAX too.\par
function doShowAll() \{\par
if (CheckBrowser()) \{\par
var key = "";\par
var list = "<tr><th>Item</th><th>Value</th></tr>\\n";\par
var i = 0;\par
//for more advance feature, you can set cap on max items in the cart\par
for (i = 0; i <= localStorage.length-1; i++) \{\par
key = localStorage.key(i);\par
list += "<tr><td>" + key + "</td>\\n<td>"\par
+ localStorage.getItem(key) + "</td></tr>\\n";\par
\}\par
//if no item exists in the cart\par
if (list == "<tr><th>Item</th><th>Value</th></tr>\\n") \{\par
list += "<tr><td><i>empty</i></td>\\n<td><i>empty</i></td></tr>\\n";\par
\}\par
//bind the data to html table\par
//you can use jQuery too....\par
document.getElementById('list').innerHTML = list;\par
\} else \{\par
alert('Cannot save shopping list as your browser does not support HTML 5');\par
\}\par
\}\par
/*\par
=====> Checking the browser support\par
//this step may not be required as most of modern browsers do support HTML5\par
*/\par
//below function may be redundant\par
function CheckBrowser() \{\par
if ('localStorage' in window && window['localStorage'] !== null) \{\par
// we can use localStorage object to store data\par
return true;\par
\} else \{\par
\f1\fs22 Page 63\par
\f3\fs28 return false;\par
\}\par
\}\par
